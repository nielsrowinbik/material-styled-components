{
	"name": "material-styled-components",
	"version": "0.0.5",
	"description": "Styled Components that implement Google's Material Design",
	"bugs": "https://github.com/nielsrowinbik/material-styled-components/issues",
	"license": "MIT",
	"author": "Niels Bik <hey@nielsbik.nl> (https://nielsbik.nl)",
	"main": "./src/index.js",
	"repository": "nielsrowinbik/material-styled-components",
	"scripts": {
		"test": "eslint ./src/**",
		"prebuild": "rimraf ./build",
		"build": "babel ./src --out-dir ./build --ignore *.test.js",
		"postbuild": "babel-node ./scripts/copy-files.js",
		"dev": "babel ./src --out-dir ./build --ignore *.test.js --watch",
		"docs:test": "eslint ./docs/src/**"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"eslint": "^4.10.0",
		"eslint-config-synacor": "^2.0.2",
		"fs-extra": "^4.0.2",
		"rimraf": "^2.6.2"
	},
	"dependencies": {
		"color": "^2.0.1",
		"prop-types": "^15.6.0",
		"react": "^16.0.0",
		"react-dom": "^16.1.1",
		"react-transition-group": "^2.2.1",
		"styled-components": "^2.2.3"
	},
	"eslintConfig": {
		"extends": "eslint-config-synacor"
	}
}
